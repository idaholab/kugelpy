source /etc/profile.d/modules.sh
module load use.exp_ctl
module load serpent/2.1.32-intel-19.0
module load serpent/2.1.32
#
cd $PBS_O_WORKDIR
#
JOB_NUM=${PBS_JOBID%\.*}
if [ $PBS_O_WORKDIR != $HOME ]
then
ln -s $HOME/$PBS_JOBNAME.o$JOB_NUM $PBS_JOBNAME.o$JOB_NUM
fi
#
export TMPDIR=<tmpdir_from_tempfile_module>
#
#
#
mpiexec sss2 pbr_run_in.inp -omp 48

#
#
if [ $PBS_O_WORKDIR != $HOME ]
then
rm $PBS_O_WORKDIR/$PBS_JOBNAME.o$JOB_NUM
